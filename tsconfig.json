{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@app/*": ["./app/*"],
      "@search-list/*": ["./app/(dashboard)/protocols/search-list/*"],
      "@user/*": ["./app/(dashboard)/users/*"],
      "@towers/*": ["./app/(dashboard)/towers/*"],
      "@permissions/*": ["./app/(dashboard)/settings/permissions/*"],
      "@onboarding/*": ["./app/(authentication)/onboarding/*"],
      "@organization/*": ["./app/(authentication)/organization/*"],
      "@towerdays/*": ["./app/(dashboard)/tower-days/*"],
      "@accomodations/*": ["./app/(dashboard)/accomodations/*"],
      "@settings/*": ["./app/(dashboard)/settings/*"],
      "@users/*": ["./app/(dashboard)/users/*"],
      "@components/*": ["./components/*"],
      "@server/*": ["./server/*"],
      "@schemas/*": ["./schemas/*"],
      "@constants/*": ["./constants/*"],
      "@hooks/*": ["./hooks/*"],
      "@utils/*": ["./utils/*"],
      "@styles/*": ["./styles/*"],
      "@type/*": ["./type/*"],
      "@services/*": ["./services/*"],
      "@data/*": ["./data/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
